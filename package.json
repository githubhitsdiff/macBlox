{
  "name": "appleblox",
  "version": "0.8.5",
  "description": "Lightweight and fast Roblox launcher for MacOS",
  "main": "frontend/src/windows/main/main.ts",
  "scripts": {
    "vite:dev": "vite", // Starts the Vite development server
    "dev": "bun run scripts/package/dev.ts", // Runs development script via Bun
    "package": "bun run scripts/build/ts/index.ts", // Packages the app
    "release": "bun run package && bun run scripts/build/ts/dmg.ts", // Prepares release (includes packaging and dmg build)
    "format": "prettier --write ./", // Formats code with Prettier
    "neu:empty": "" // Placeholder for any neutralino commands
  },
  "keywords": [
    "svelte",
    "neutralino",
    "webdev",
    "ts",
    "roblox"
  ],
  "type": "module",
  "author": "OrigamingWasTaken",
  "license": "MIT",
  "devDependencies": {
    "@neutralinojs/neu": "^10.1.0", // Neutralino.js CLI tool
    "@sveltejs/vite-plugin-svelte": "^3.0.2", // Vite plugin for Svelte
    "@tsconfig/svelte": "^5.0.2", // TypeScript configuration for Svelte
    "@types/dompurify": "^3.0.5", // TypeScript types for DomPurify
    "@types/path-browserify": "^1.0.2", // TypeScript types for path-browserify
    "@types/semver-compare": "^1.0.3", // TypeScript types for semver-compare
    "@types/signale": "^1.4.7", // TypeScript types for Signale (logging)
    "@types/tar": "^6.1.11", // TypeScript types for tar
    "autoprefixer": "^10.4.19", // Auto-prefix CSS for cross-browser compatibility
    "postcss": "^8.4.38", // PostCSS for processing CSS
    "prettier": "^3.3.3", // Prettier for code formatting
    "prettier-plugin-svelte": "^3.2.6", // Prettier plugin for Svelte files
    "resedit": "^2.0.0", // Resource editor
    "signale": "^1.4.0", // Logging tool
    "svelte": "^4.2.11", // Svelte framework
    "svelte-check": "^3.6.4", // Svelte type checker
    "tailwindcss": "^3.4.3", // Tailwind CSS framework
    "tar": "^6.2.0", // Tar utility for handling tar files
    "terser": "^5.34.1", // JavaScript minifier
    "tslib": "^2.6.2", // TypeScript library
    "typescript": "^5.3.3", // TypeScript
    "vite": "4.4.9", // Vite build tool
    "vite-plugin-checker": "^0.7.2", // Vite plugin for type-checking
    "vite-plugin-html": "^3.2.2" // Vite plugin for HTML file handling
  },
  "dependencies": {
    "@neutralinojs/lib": "^5.1.0", // Neutralino.js library
    "@tailwindcss/typography": "^0.5.15", // Tailwind typography plugin
    "@types/bun": "^1.1.9", // TypeScript types for Bun
    "bits-ui": "^0.21.16", // UI components library
    "clsx": "^2.1.1", // Utility for conditional class names
    "cmdk-sv": "^0.0.18", // Command palette for Svelte
    "dompurify": "^3.1.7", // DOM sanitization library
    "hotkeys-js": "^3.13.7", // Keyboard shortcut handling
    "json-beautify": "^1.1.1", // Beautify JSON data
    "lucide-svelte": "^0.453.0", // Lucide icon library for Svelte
    "marked": "^14.1.3", // Markdown parser
    "mode-watcher": "^0.3.1", // Watches for mode changes
    "path-browserify": "^1.0.1", // Node.js path module for browsers
    "semver-compare": "^1.0.0", // Compares version strings
    "svelte-sonner": "^0.3.21", // Svelte notifications
    "tailwind-merge": "^2.5.2", // Merges Tailwind CSS classes
    "tailwind-variants": "^0.2.1" // Tailwind utility for variants
  },
  "trustedDependencies": [
    "bufferutil", // Buffer utilities
    "core-js", // Polyfills for ECMAScript features
    "es5-ext", // Polyfills for ES5 extensions
    "esbuild", // Fast JavaScript bundler
    "svelte-preprocess", // Preprocessing for Svelte (SCSS, TypeScript, etc.)
    "utf-8-validate" // Validation for UTF-8 encoded data
  ]
}
